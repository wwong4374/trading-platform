{
  "name": "trading-platform",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:wwong4374/trading-platform.git",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.7",
    "knex": "^3.1.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/knex": "^0.16.1",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "db-create": "ts-node createDB.ts",
    "db-migrate": "knex --knexfile knexfile.ts migrate:latest",
    "db-migrate-make": "knex --knexfile knexfile.ts migrate:make",
    "db-migrate-rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "db-seed": "knex --knexfile knexfile.ts seed:run",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts --fix"
  },
  "pre-commit": [
    "format",
    "lint"
  ]
}
