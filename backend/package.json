{
  "name": "trading-platform",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:wwong4374/trading-platform.git",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.7",
    "knex": "^3.1.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/knex": "^0.16.1",
    "@types/node": "^22.10.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "db-create": "ts-node createDB.ts",
    "db-migrate": "knex --knexfile knexfile.ts migrate:latest",
    "db-migrate-make": "knex --knexfile knexfile.ts migrate:make",
    "db-migrate-rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "db-seed": "knex --knexfile knexfile.ts seed:run",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "start": "ts-node src/index.ts"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
